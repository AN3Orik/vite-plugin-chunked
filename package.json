{
  "name": "@anzohost/vite-plugin-chunked",
  "version": "1.0.4",
  "description": "Vite plugin that splits assets into chunks and reassembles them via Service Worker to bypass ISP throttling",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "chunked",
    "service-worker",
    "dpi-bypass",
    "cdn",
    "download-manager"
  ],
  "author": "AN3Orik",
  "license": "CC-BY-NC-4.0",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-toastify": "^9.0.0 || ^10.0.0 || ^11.0.0",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-toastify": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "terser": "^5.44.1",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^6.0.0"
  }
}
